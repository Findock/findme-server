# Server start environment (dev | production)
ENV = dev

# Server will listen on that port
LISTEN_PORT = 3005

# Encryption key (used for database hashing)
ENCRYPT_KEY =

## Database configuration
DB_ENGINE = mysql
DB_HOST = find-me-db # For docker use find-me-db
DB_PORT = 3306
DB_USER = root
DB_PASSWORD = root
DB_DATABASE_NAME = find-me-dev
DB_PORT_DOCKER_LISTEN = 3006 # Outside from docker database connection port
DB_AUTO_MIGRATE = false # Controls database auto-migrations
DB_SEED = false # Controls database seeding (example autogenerated records in tables)

# Backend root url (ex. http://192.168.0.2:3005/)
ROOT_URL = http://

# Mailer host for sending emails (ex. smtp.gmail.com)
SMTP_HOST = smtp.gmail.com

# Mailer user for sending emails
SMTP_USER =

# Mailer password for sending emails
SMTP_PASSWORD =
